{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "build:firefox": {
      "dependsOn": [
        "^build:firefox"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "debug": {
      "cache": false,
      "persistent": true
    },
    "debug:firefox": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:firefoxck": {
      "cache": false,
      "persistent": true
    },
    "package": {
      "dependsOn": [
        "^package"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "package:firefox": {
      "dependsOn": [
        "^package:firefox"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "//#format": {
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "cache": false
    },
    "format": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "cache": false
    },
    "//#format:fix": {
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "cache": false
    },
    "format:fix": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "cache": false
    }
  }
}
